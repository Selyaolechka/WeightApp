<Window x:Class="WeightApp.Client.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeightApp.Client"
        mc:Ignorable="d"
        Title="Login" Height="166.009" Width="215.313">
    <StackPanel RenderTransformOrigin="0.5,0.5">
        <StackPanel.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform AngleX="0.862"/>
                <RotateTransform/>
                <TranslateTransform X="1.029"/>
            </TransformGroup>
        </StackPanel.RenderTransform>
        <TextBlock Text="Email"/>
        <TextBox Text="user@example.com"/>
